{
  "name": "Ethan Mollick RSS to ntfy.sh",
  "nodes": [
    {
      "parameters": {},
      "id": "start-node-id",
      "name": "Start",
      "type": "n8n-nodes-base.start",
      "typeVersion": 1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "url": "https://www.oneusefulthing.org/feed"
      },
      "id": "rss-node-id",
      "name": "RSS Feed Read",
      "type": "n8n-nodes-base.rssFeedRead",
      "typeVersion": 1,
      "position": [460, 300],
      "trigger": true
    },
    {
      "parameters": {
        "method": "POST",
        "url": "https://ntfy.sh/my-n8n-test-feed-ax78",
        "sendBody": true,
        "body": "={{ $json.title }}",
        "options": {
          "headerParameters": {
            "parameters": [
              {
                "name": "Title",
                "value": "={{ $json.title }}"
              },
              {
                "name": "Click",
                "value": "={{ $json.link }}"
              },
              {
                "name": "Tags",
                "value": "newspaper"
              }
            ]
          }
        }
      },
      "id": "http-node-id",
      "name": "Send to ntfy.sh",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [680, 300]
    }
  ],
  "connections": {
    "RSS Feed Read": {
      "main": [
        [
          {
            "node": "Send to ntfy.sh",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
}
